import bpy
import bpy_extras

scn = bpy.context.scene
cam = bpy.data.objects['Camera']
for o in bpy.data.objects:
    if o.name.find('Cube')<0 : continue
    lo = o.location
    co = bpy_extras.object_utils.world_to_camera_view(scn, cam, lo)
    co.y = 1.0 - co.y
    print(co.x,co.y)
